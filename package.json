    "scripts": {
        "start": "ts-node -r node-localstorage/register ./dist/server.js",
        "start:watch": "nodemon ./dist/server.js",
        "build": "npx webpack",
        "test": "ENSEMBLE_MANAGER_CONFIG_FILE=src/config/config.json  jest",
        "lint-staged": "lint-staged",
